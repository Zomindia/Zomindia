<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/subcategoryRelative"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:ignore="MissingPrefix">

    <LinearLayout
        android:id="@+id/llParent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/dp_7"
        android:background="@color/white"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="@dimen/dp_130"
                android:layout_height="@dimen/dp_120"
                android:layout_marginRight="@dimen/dp_10"
                android:orientation="vertical">

                <androidx.cardview.widget.CardView
                    android:layout_width="@dimen/dp_120"
                    android:layout_height="@dimen/dp_110"
                    android:layout_margin="@dimen/dp_10"
                    android:elevation="@dimen/dp_10"
                    card_view:cardCornerRadius="@dimen/dp_7">


                    <ImageView
                        android:id="@+id/image_sub"
                        android:layout_width="@dimen/dp_120"
                        android:layout_height="@dimen/dp_110"
                        android:scaleType="fitXY"
                        android:src="@mipmap/carwash" />

                </androidx.cardview.widget.CardView>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_5"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    >

                    <TextView
                        android:id="@+id/prd_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:textColor="@color/black"
                        android:textSize="@dimen/dp_16"
                        android:hint="1 Hour proccess" />

                    <TextView
                        android:id="@+id/prd_price"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginLeft="@dimen/dp_5"
                        android:layout_marginRight="@dimen/dp_5"
                        android:hint="3400"
                        android:textColor="@color/black"
                        android:textSize="@dimen/dp_16" />
                </RelativeLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <RelativeLayout
                        android:id="@+id/relative_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_7"
                        >
                        <TextView
                            android:id="@+id/info"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Info"

                            android:textStyle="bold"
                            android:textSize="@dimen/dp_16"
                            android:textColor="@color/green"/>

                        <ImageView
                            android:id="@+id/set_img_info"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:scaleType="fitStart"
                            android:layout_marginLeft="@dimen/dp_10"
                            android:layout_toRightOf="@+id/info"
                            android:background="@drawable/ic_information"/>

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:layout_margin="@dimen/dp_5"
                        >

                        <ImageView
                            android:id="@+id/arrowright"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:paddingLeft="@dimen/dp_20"
                            android:paddingRight="@dimen/dp_10"
                            android:scaleType="fitStart"
                            android:layout_gravity="right"
                            android:src="@drawable/ic_rightarrow"/>

                    </RelativeLayout>
                </LinearLayout>
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/sct_information"

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_2"
                    android:layout_marginRight="@dimen/dp_2"
                    android:textSize="@dimen/dp_14"
                    android:hint="dsuh"
                    android:visibility="gone"
                    />
                </RelativeLayout>
                <LinearLayout
                    android:id="@+id/linear_viewprice_less"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="@dimen/dp_2"
                    android:visibility="gone"
                    >
                    <TextView
                        android:id="@+id/pdf_view"

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dp_5"

                        android:gravity="left"
                        android:text="View Pricing"
                        android:textColor="@color/green"
                        android:textSize="@dimen/dp_16"
                        android:layout_marginTop="@dimen/dp_2"
                        android:textStyle="bold"

                        />


                <TextView
                    android:id="@+id/less_selct"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/dp_5"
                    android:text="Less.."
                    android:layout_marginTop="@dimen/dp_2"
                    android:textStyle="bold"
                    android:textColor="@color/green"
                    android:textSize="@dimen/dp_14"
                    android:gravity="right"
                    android:layout_weight="2"
                    android:layout_below="@+id/sct_information"
                    android:layout_alignParentRight="true"
                   />
                </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_5"
                    android:layout_marginBottom="@dimen/dp_5"
                    android:orientation="horizontal">

                    <TextView

                        fontPath="@string/font_quicksand_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Rating"
                        android:textStyle="bold"
                        android:textColor="@color/rattingTextColor"
                        android:textSize="@dimen/dp_12"

                        />

                    <RatingBar
                        android:id="@+id/ratingBar"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:theme="@style/RatingBar"
                        android:isIndicator="true"
                        android:clickable="false"
                        android:numStars="5"

                        android:layout_gravity="center"
                        style="@style/Widget.AppCompat.RatingBar.Small"
                        />


                    <TextView
                        android:id="@+id/ratingpoint"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_4"
                        android:layout_marginBottom="@dimen/dp_5"
                        android:text="0"
                        android:layout_marginLeft="@dimen/dp_10"
                        android:textColor="@color/green"
                        android:textSize="@dimen/dp_16"
                        android:visibility="gone"/>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_20"
                    android:orientation="horizontal"
                    android:visibility="gone"
                    android:layout_gravity="center"
                    android:weightSum="6">

                    <ImageView
                        android:id="@+id/prd_down"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.5"
                        android:layout_marginTop="@dimen/dp_3"
                        android:src="@drawable/ic_dowarrow"

                        />

                    <ImageView
                        android:id="@+id/prd_like"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dp_10"
                        android:layout_marginTop="@dimen/dp_3"
                        android:layout_weight="0.5"
                        android:src="@drawable/ic_like"
                        />

                    <TextView
                        android:id="@+id/prd_rating"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dp_10"
                        android:layout_weight="2.5"
                        android:textSize="@dimen/dp_16"
                        android:textColor="@color/red"
                        android:text="Rating" />

                    <ImageView
                        android:id="@+id/prd_minus"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dp_10"
                        android:layout_weight="0.3"
                        android:src="@drawable/ic_minus"
                        android:layout_marginRight="@dimen/dp_5" />

                    <TextView
                        android:id="@+id/prd_value"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.5"
                        android:gravity="center"
                        android:textSize="@dimen/dp_16"
                        android:text="100" />

                    <ImageView
                        android:id="@+id/prd_plus"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.5"
                        android:layout_marginLeft="@dimen/dp_5"
                        android:layout_marginRight="@dimen/dp_10"
                        android:src="@drawable/ic_plus" />

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>


    </LinearLayout>

