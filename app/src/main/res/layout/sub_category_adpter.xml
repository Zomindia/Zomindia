<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/subcategoryRelative"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:ignore="MissingPrefix">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_5"
        android:layout_marginRight="@dimen/dp_5"
        android:layout_marginBottom="@dimen/dp_7"
        android:background="@color/white"
        android:elevation="20dp"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="@dimen/dp_130"
                android:layout_height="@dimen/dp_120"
                android:layout_marginRight="@dimen/dp_10"
                android:orientation="vertical">


                <ImageView
                    android:id="@+id/image"
                    android:layout_marginTop="@dimen/dp_5"
                    android:layout_marginLeft="@dimen/dp_5"
                    android:layout_width="@dimen/dp_120"
                    android:layout_height="@dimen/dp_110"
                    android:scaleType="fitXY"
                    android:src="@mipmap/carwash" />

                <androidx.cardview.widget.CardView
                    android:layout_width="@dimen/dp_120"
                    android:layout_height="@dimen/dp_110"
                    android:elevation="@dimen/dp_10"
                    android:layout_margin="@dimen/dp_10"
                    card_view:cardCornerRadius="@dimen/dp_7">




                </androidx.cardview.widget.CardView>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_5"
                android:orientation="vertical">


                <TextView
                    android:layout_marginLeft="@dimen/dp_5"
                    android:id="@+id/name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    android:hint="7 service"
                    android:layout_marginTop="@dimen/dp_10"
                    android:textStyle="bold"
                    android:textColor="@color/black"
                    android:textSize="@dimen/dp_16" />


                <LinearLayout
                    android:layout_marginTop="@dimen/dp_5"
                    android:padding="@dimen/dp_5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <RelativeLayout
                        android:id="@+id/relative_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        >

                        <TextView
                            android:id="@+id/info"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Info"
                            android:textColor="@color/green"
                            android:textSize="@dimen/dp_16"
                            android:textStyle="bold" />

                        <ImageView
                            android:id="@+id/img_info"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dp_10"
                            android:layout_toRightOf="@+id/info"
                            android:background="@drawable/ic_information"
                            android:scaleType="fitStart" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/relative_arrow"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:layout_margin="@dimen/dp_5"
                        >

                        <ImageView
                            android:id="@+id/arrowright"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:scaleType="fitStart"
                            android:paddingLeft="@dimen/dp_20"
                            android:paddingRight="@dimen/dp_10"
                            android:layout_gravity="right"
                            android:src="@drawable/ic_rightarrow"/>

                    </RelativeLayout>
                </LinearLayout>


                <TextView
                    android:id="@+id/information"
                    fontPath="@string/font_quicksand_regular"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_2"
                    android:layout_marginRight="@dimen/dp_2"
                    android:textSize="@dimen/dp_14"
                    android:visibility="gone"
                    />

                <TextView
                    android:id="@+id/less"

                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/dp_5"
                    android:gravity="right"
                    android:text="Less.."
                    android:textStyle="bold"
                    android:textColor="@color/green"
                    android:textSize="@dimen/dp_14"
                    android:visibility="gone"
                   />


                <LinearLayout
                    android:padding="@dimen/dp_5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:layout_marginBottom="@dimen/dp_5"
                    android:orientation="horizontal">

                    <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Rating"
                    android:textStyle="bold"
                        android:layout_gravity="center"
                    android:textColor="@color/rattingTextColor"
                    android:textSize="@dimen/dp_12"

                    />

                    <RatingBar

                        android:id="@+id/ratingBar"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:theme="@style/RatingBar"
                        android:isIndicator="true"
                        android:clickable="false"
                        android:numStars="5"
                        android:layout_gravity="center"
                        style="@style/Widget.AppCompat.RatingBar.Small"
/>


                    <TextView
                        android:id="@+id/ratingpoint"
                        fontPath="@string/font_quicksand_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_4"
                        android:layout_marginBottom="@dimen/dp_5"
                        android:text="0"
                        android:layout_marginLeft="@dimen/dp_10"
                        android:textColor="@color/green"
                        android:textSize="@dimen/dp_16"
                        android:visibility="gone"/>


                </LinearLayout>

            </LinearLayout>

        </LinearLayout>


    </LinearLayout>

